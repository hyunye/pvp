{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "lib": ["ESNext","DOM", "DOM.Iterable"],
    "skipLibCheck": true,

    /* Linting */
    "strict": true,
    "erasableSyntaxOnly": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "resolveJsonModule": true,
  },
  "exclude": ["vite.config.ts"]
}
